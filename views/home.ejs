<%- include('./partials/header'); -%>
    <h1>
        <a href="/">
            Home
        </a>
    </h1>
    <p>
        <%=startingContent%>
    </p>
    <% posts.forEach( post=>{ %>
        <h1>
            <a href="/posts/<%=post._id%>">
                <%=post.title%>
            </a>
        </h1>
        <p>
            <%=post.content.substring(0,100) + "..." %>
                <a href="/posts/<%=post._id%>">Read More</a>
        </p>
        <%});%>
            <%- include('./partials/footer'); -%>